{
  "name": "dev_server",
  "version": "1.0.0",
  "description": "Easy frontend develop server with express, jade and stylus",
  "dependencies": {
    "concurrently": "^2.0.0",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "milligram": "^1.1.0",
    "pug": "^2.0.0-alpha6",
    "socket.io": "^1.4.8",
    "stylus": "^0.54.3"
  },
  "scripts": {
    "server": "concurrently --kill-others 'stylus --watch --compress main.styl' 'jade --watch --pretty index.jade' 'node server.js'"
